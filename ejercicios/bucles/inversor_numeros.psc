Proceso inversor_numeros
	//datos
	definir a,b,c,d como entero
	//entrada
	escribir sin saltar "primer numero: "
	leer a
	escribir sin saltar "segundo numero: "
	leer b
	escribir sin saltar "tercer numero: "
	leer c
	escribir sin saltar "ultimo numero: "
	leer d
	//salida
	escribir d,c,b,a
FinProceso
